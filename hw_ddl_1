create table if not exists track(
  id serial primary key,
  name varchar(40),
  duratation numeric
);

create table if not exists genre(
  id serial primary key,
  name varchar(40)
);

create table if not exists albums(
  id serial primary key,
  name varchar(40),
  year date,
  track integer unique not null references track(id)
);

create table if not exists executor(
  id serial primary key,
  name varchar(40),
  album_id integer unique not null references albums(id),
  genre_id integer unique not null references genre(id)
);